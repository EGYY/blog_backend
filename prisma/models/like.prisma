model Like {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  userId    String
  articleId String
  article   Article  @relation(fields: [articleId], references: [id])
  user      User     @relation(fields: [userId], references: [id])

  @@unique([userId, articleId])
  @@map("like")
}
